{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Azure": "Warning",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "value-from-config",
    "Ignore": {
      "Requests": {
        "Names": [],
        "Paths": [
          "/swagger",
          "/health",
          "/healthcheck",
          "/healthz"
        ]
      },
      "Dependencies": {
        "Names": []
      }
    }
  },
  "OpenTelemetry": {
    "AddConsoleExporter": false,
    "AddHttpClientInstrumentation": true,
    "AddAspNetCoreInstrumentation": true,
    "AddSqlClientInstrumentation": false
  },
  "WillowContext": {
    "EnvironmentConfiguration": {
      "ShortName": ""
    },
    "RegionConfiguration": {
      "ShortName": ""
    },
    "StampConfiguration": {
      "Name": ""
    },
    "CustomerInstanceConfiguration": {
      "CustomerSalesId": "",
      "CustomerInstanceName": ""
    }
  },
  "ConnectionStrings": {
    "InsightDb": "[value from key vault]"
  },
  "Auth0": {
    "Domain": "[value from app service setting]",
    "Audience": "[value from app service setting]",
    "ClientId": "[value from app service setting]"
  },
  "HttpClientFactory": {
    "DigitalTwinCore": {
      "BaseAddress": "[value from app service setting]",
      "Authentication": {
        "Scheme": "MachineToMachine",
        "Domain": "[value from app service setting]",
        "ClientId": "[value from app service setting]",
        "ClientSecret": "[value from app service setting]",
        "Audience": "[value from app service setting]"
      }
    },
    "WorkflowCore": {
      "BaseAddress": "[value from app service setting]",
      "Authentication": {
        "Scheme": "MachineToMachine",
        "Domain": "[value from app service setting]",
        "ClientId": "[value from app service setting]",
        "ClientSecret": "[value from app service setting]",
        "Audience": "[value from app service setting]"
      }
    }
  },
  "Segment": {
    "ApiKey": "[value from app service setting]"
  },
  "BackgroundJobOptions": {
    "EnableProcess": true,
    "BatchSize": 100
  },
  "EnableSwagger": "[value from app service setting]",
  "EnableEnhancedLogging": "[value from app service setting]",
  "InspectionOptions": {
    "RuleName": "Inspection Value Out of Range",
    "RuleId": "inspection-value-out-of-range-"
  },
  "WillowActivateOptions": {
    "AppId": "[value from app service setting]",
    "AppName": "Willow Activate"
  },
  "MappedIntegrationConfiguration": {
    "AppId": "[value from app service setting]",
    "AppName": "Mapped"
  },
  "ServiceBusOptions": {
    "ConnectionString": "[value from app service setting]",
    "NotificationTopicName": "[value from app service setting]"
  },
  "IsNotificationEnabled" :  false
}
