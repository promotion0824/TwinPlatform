{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Azure": "Warning",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "value-from-config",
    "Ignore": {
      "Requests": {
        "Names": [],
        "Paths": [
          "/swagger",
          "/health",
          "/healthcheck",
          "/healthz"
        ]
      },
      "Dependencies": {
        "Names": []
      }
    }
  },
  "OpenTelemetry": {
    "AddConsoleExporter": false,
    "AddHttpClientInstrumentation": true,
    "AddAspNetCoreInstrumentation": true,
    "AddSqlClientInstrumentation": false
  },
  "WillowContext": {
    "EnvironmentConfiguration": {
      "ShortName": ""
    },
    "RegionConfiguration": {
      "ShortName": ""
    },
    "StampConfiguration": {
      "Name": ""
    },
    "CustomerInstanceConfiguration": {
      "CustomerSalesId": "",
      "CustomerInstanceName": ""
    }
  },
  "ConnectionStrings": {
    "WorkflowDb": "[value from app service setting or keyvault]"
  },
  "HttpClientFactory": {
    "ImageHub": {
      "BaseAddress": "[value from app service setting]",
      "Authentication": {
        "Scheme": "PassDownAuthorization"
      },
      "RequiresServiceKey": true
    },
    "DirectoryCore": {
      "BaseAddress": "[value from app service setting]",
      "Authentication": {
        "Scheme": "MachineToMachine",
        "Domain": "[value from app service setting]",
        "ClientId": "[value from app service setting]",
        "ClientSecret": "[value from app service setting]",
        "Audience": "[value from app service setting]"
      }
    },
    "SiteCore": {
      "BaseAddress": "[value from app service setting]",
      "Authentication": {
        "Scheme": "MachineToMachine",
        "Domain": "[value from app service setting]",
        "ClientId": "[value from app service setting]",
        "ClientSecret": "[value from app service setting]",
        "Audience": "[value from app service setting]"
      },
      "RequiresServiceKey": true
    },
    "DigitalTwinCore": {
      "BaseAddress": "[value from app service setting]",
      "Authentication": {
        "Scheme": "MachineToMachine",
        "Domain": "[value from app service setting]",
        "ClientId": "[value from app service setting]",
        "ClientSecret": "[value from app service setting]",
        "Audience": "[value from app service setting]"
      }
    },
    "InsightCore": {
      "BaseAddress": "[value from app service setting]",
      "Authentication": {
        "Scheme": "MachineToMachine",
        "Domain": "[value from app service setting]",
        "ClientId": "[value from app service setting]",
        "ClientSecret": "[value from app service setting]",
        "Audience": "[value from app service setting]"
      }
    },
    "MarketPlaceCore": {
      "BaseAddress": "[value from app service setting]",
      "Authentication": {
        "Scheme": "MachineToMachine",
        "Domain": "",
        "ClientId": "",
        "ClientSecret": "",
        "Audience": ""
      },
      "RequiresServiceKey": true
    }
  },
  "Auth0": {
    "Domain": "[value from app service setting]",
    "Audience": "[value from app service setting]",
    "ClientId": "[value from app service setting]"
  },
  "EnableSwagger": "[value from app service setting]",
  "CommandPortalBaseUrl": "[value from app service setting]",
  "MobilePortalBaseUrl": "[value from app service setting]",
  "MessageQueue": {
    "TicketInsightStatusQueue": "ticketinsightstatus",
    "CommServiceQueue": "commservice"
  },
  "BackgroundJobOptions": {
    "Inspection": {
      "EnableProcess": false,
      "BatchSize": 100
    },
    "Ticket": {
      "EnableProcess": true,
      "BatchSize": 100
    },
    "InspectionGenerateRecords": {
      "EnableProcess": false,
      "CronExpression": "0 */5 * * * ?"
    },
    "SendDailyInspectionReport": {
      "EnableProcess": false,
      "CronExpression": "0 0 0 * * ?"
    },
    "SchedulerPickup": {
      "EnableProcess": false,
      "CronExpression": "0 */15 * * * ?"
    }
  },
  // this customer Id c182bab8-75a3-4097-b18d-01415964066d = Hollywood Park
  // d11ec74d-8136-4e11-b459-94becb06d380 = Walmart
  // 013573ca-ea3c-4b9c-8ce8-68bb384e0c02 = Walmart Retail
  "NotificationExcludedCustomers": [
    "c182bab8-75a3-4097-b18d-01415964066d",
    "d11ec74d-8136-4e11-b459-94becb06d380",
    "013573ca-ea3c-4b9c-8ce8-68bb384e0c02"
  ],
  "TicketEventsTopicName": "ticketeventstopic",
  "MappedIntegrationConfiguration": {
    "IsEnabled": false
  }
}
