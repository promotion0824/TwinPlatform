{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Azure": "Warning",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "value-from-config",
    "Ignore": {
      "Requests": {
        "Names": [],
        "Paths": [
          "/swagger",
          "/health",
          "/healthcheck",
          "/healthz"
        ]
      },
      "Dependencies": {
        "Names": []
      }
    }
  },
  "OpenTelemetry": {
    "AddConsoleExporter": false,
    "AddHttpClientInstrumentation": true,
    "AddAspNetCoreInstrumentation": true
  },
  "WillowContext": {
    "EnvironmentConfiguration": {
      "ShortName": ""
    },
    "RegionConfiguration": {
      "ShortName": ""
    },
    "StampConfiguration": {
      "Name": ""
    },
    "CustomerInstanceConfiguration": {
      "CustomerSalesId": "",
      "CustomerInstanceName": ""
    }
  },
  "ConnectionStrings": {
    "SiteDb": "[from app setting]"
  },
  "HttpClientFactory": {
    "ImageHub": {
      "BaseAddress": "[from app setting]",
      "Authentication": {
        "Scheme": "TokenFromCookie"
      },
      "RequiresServiceKey": true
    },
    "DigitalTwinCore": {
      "BaseAddress": "[from app setting]",
      "Authentication": {
        "Scheme": "MachineToMachine",
        "Domain": "",
        "ClientId": "",
        "ClientSecret": "",
        "Audience": ""
      },
      "RequiresServiceKey": true
    }
  },
  "EnableSwagger": "[from app setting]",
  "Integrations": {
    "LogoStorageUrl": "[from app setting]",
    "FloorPlanStorageUrl": "[from app setting]"
  },
  "Auth0": {
    "Domain": "[from app setting]",
    "Audience": "[from app setting]"
  },
  "FloorModuleOptions": {
    "Modules2D": {
      "AllowedExtensions": [
        ".png"
      ],
      "MaxSizeBytes": 2097152,
      "MaxWidth": 4096,
      "MaxHeight": 3072
    },
    "Modules3D": {
      "MaxSizeBytes": 5242880
    }
  },
  "ForgeOptions": {
    "ClientId": "[from app setting]",
    "ClientSecret": "[from app setting]",
    "Scope": [
      "viewables:read"
    ],
    "TokenEndpoint": "https://developer.api.autodesk.com/authentication/v1/authenticate"
  },
  "BackgroundJobOptions": {
    "Widget": {
      "EnableProcess": true,
      "BatchSize": 100
    }
  }
}
