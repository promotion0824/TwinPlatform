{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Azure": "Warning",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "value-from-config",
    "Ignore": {
      "Requests": {
        "Names": [],
        "Paths": [
          "/swagger",
          "/health",
          "/healthcheck",
          "/healthz"
        ]
      },
      "Dependencies": {
        "Names": []
      }
    }
  },
  "OpenTelemetry": {
    "AddConsoleExporter": false,
    "AddHttpClientInstrumentation": true,
    "AddAspNetCoreInstrumentation": true
  },
  "WillowContext": {
    "EnvironmentConfiguration": {
      "ShortName": ""
    },
    "RegionConfiguration": {
      "ShortName": ""
    },
    "StampConfiguration": {
      "Name": ""
    },
    "CustomerInstanceConfiguration": {
      "CustomerSalesId": "",
      "CustomerInstanceName": ""
    }
  },
  "HttpClientFactory": {
    "DirectoryCore": {
      "BaseAddress": "[value from app service setting]",
      "Authentication": {
        "Scheme": "TokenFromCookie"
      },
      "RequiresServiceKey": true
    },
    "RegionalDirectoryCore": {
      "BaseAddress": "[value from app service setting]",
      "Authentication": {
        "Scheme": "MachineToMachinePerRegion"
      },
      "RequiresServiceKey": true
    }
  },
  "Regions": {},
  "EnableSwagger": "[value from app service setting]"
}