{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Azure": "Warning",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "value-from-config",
    "Ignore": {
      "Requests": {
        "Names": [],
        "Paths": [
          "/swagger",
          "/health",
          "/healthcheck",
          "/healthz"
        ]
      },
      "Dependencies": {
        "Names": []
      }
    }
  },
  "OpenTelemetry": {
    "AddConsoleExporter": false,
    "AddHttpClientInstrumentation": true,
    "AddAspNetCoreInstrumentation": true
  },
  "WillowContext": {
    "EnvironmentConfiguration": {
      "ShortName": ""
    },
    "RegionConfiguration": {
      "ShortName": ""
    },
    "StampConfiguration": {
      "Name": ""
    },
    "CustomerInstanceConfiguration": {
      "CustomerSalesId": "",
      "CustomerInstanceName": ""
    }
  },
  "ConnectionStrings": {
    "DigitalTwinDb": "[value from key vault]"
  },
  "HttpClientFactory": {
    "DirectoryCore": {
      "BaseAddress": "[value from app service setting]",
      "Authentication": {
        "Scheme": "MachineToMachine"
      },
      "RequiresServiceKey": true
    }
  },
  "EnableSwagger": "[value from app service setting]",
  "Azure": {
    "BlobStorage": {
      "AccountName": "[value from app service setting]",
      "ContainerName": "[value from app service setting]",
      "Url": "[value from app service setting]"
    }
  },
  "Auth0": {
    "Audience": "[value from app service setting]",
    "ClientId": "[value from app service setting]",
    "Domain": "[value from app service setting]"
  },
  "ServiceKeyAuth": {
    "ServiceKey1": "secret"
  },
  "MaxTwinsAllowedToQuery": 50,
  "EnableServiceKeyAuthMiddleware": true
}