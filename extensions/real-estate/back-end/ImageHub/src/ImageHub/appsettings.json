{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "Azure": {
    "KeyVault": {
      "KeyVaultName": "[value from app service setting]"
    }
  },
  "Storages": {
    "OriginalImage": {
      "AccountName": "[value from app service setting]",
      "Key": "[value from key vault]",
      "ContainerName": "original"
    },
    "CachedImage": {
      "AccountName": "[value from app service setting]",
      "Key": "[value from key vault]",
      "ContainerName": "cached"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "value-from-config",
    "Ignore": {
      "Requests": {
        "Names": [],
        "Paths": [
          "/swagger",
          "/health",
          "/healthcheck",
          "/healthz"
        ]
      },
      "Dependencies": {
        "Names": []
      }
    }
  },
  "OpenTelemetry": {
    "AddConsoleExporter": false,
    "AddHttpClientInstrumentation": true,
    "AddAspNetCoreInstrumentation": true
  },
  "WillowContext": {
    "EnvironmentConfiguration": {
      "ShortName": ""
    },
    "RegionConfiguration": {
      "ShortName": ""
    },
    "StampConfiguration": {
      "Name": ""
    },
    "CustomerInstanceConfiguration": {
      "CustomerSalesId": "",
      "CustomerInstanceName": ""
    }
  },
  "EnableSwagger": "[value from app service setting]",
  "Auth0": {
    "Domain": "[value from app service setting]",
    "Audience": "[value from app service setting]",
    "ClientId": "[value from app service setting]"
  },
  "AzureB2C": {
    "Instance": "[value from app service setting]",
    "Domain": "[value from app service setting]",
    "ClientId": "[value from app service setting]",
    "SignUpSignInPolicyId": "[value from app service setting]",
    "TenantId": "[value from app service setting]",
    "Authority": "[value from app service setting]"
  }
}