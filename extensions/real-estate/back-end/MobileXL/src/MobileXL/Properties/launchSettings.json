{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    // Note that this name is incorrect, but is being left incorrect to ensure it doesn't
    // intefere with anyone's project settings. At some point it might be worth changing
    // this to MobileXL.
    "PlatformPortalXL": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "local"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "UAT": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Uat-aue1",
        // This won't actually work, but will allow the app to start locally.
        "ServiceBusConnectionString": "Endpoint=sb://wil-uat-plt-aue1-sbns.servicebus.windows.net/;SharedAccessKeyName=Send;SharedAccessKey=some_access_key"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    }
  }
}
