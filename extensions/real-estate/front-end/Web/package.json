{
  "name": "willow-web",
  "scripts": {
    "start": "npm run platform",
    "platform": "cross-env NODE_ENV=development node runSingleTenantInstance.js --app platform --api-url https://ddk.app.willowinc.com",
    "platform-ci": "cross-env NODE_ENV=development node runSingleTenantInstance.js --app platform --api-url https://willow-ci.app-dev.willowinc.com",
    "platform-local": "cross-env NODE_ENV=development node runSingleTenantInstance.js --app platform --api-url https://localhost:5001",
    "platform-other": "cross-env NODE_ENV=development node runSingleTenantInstance.js --app platform",
    "platform-mock": "cross-env MOCK_SERVICE_WORKER=true NODE_ENV=development node runSingleTenantInstance.js --app platform",
    "platform-ephemeral": "cross-env NODE_ENV=development webpack serve --config ./packages/platform/webpack.config.js",
    "platform:type-check": "cross-env NODE_ENV=development WILLOW_ENV=uat TYPE_CHECK=true webpack serve --config ./packages/platform/webpack.config.js",
    "mobile": "cross-env NODE_ENV=development IS_MOBILE=true node runSingleTenantInstance.js --app mobile --api-url https://ddk.app.willowinc.com/mobile-web",
    "mobile-ci": "cross-env NODE_ENV=development IS_MOBILE=true node runSingleTenantInstance.js --app mobile --api-url https://willow-ci.app-dev.willowinc.com/mobile-web",
    "mobile-local": "cross-env NODE_ENV=development IS_MOBILE=true node runSingleTenantInstance.js --app mobile --api-url https://localhost:5002",
    "mobile-other": "cross-env NODE_ENV=development IS_MOBILE=true node runSingleTenantInstance.js --app mobile",
    "mobile-mock": "cross-env MOCK_SERVICE_WORKER=true NODE_ENV=development IS_MOBILE=true WILLOW_ENV=uat webpack serve --config ./packages/mobile/webpack.config.js",
    "mobile-mock-dev": "cross-env MOCK_SERVICE_WORKER=true NODE_ENV=development IS_MOBILE=true WILLOW_ENV=dev webpack serve --config ./packages/mobile/webpack.config.js",
    "build": "npm run build-platform",
    "build-platform": "cross-env NODE_ENV=production WILLOW_ENV=dev webpack --config ./packages/platform/webpack.config.js",
    "build-mobile": "cross-env NODE_ENV=production IS_MOBILE=true WILLOW_ENV=dev webpack --config ./packages/mobile/webpack.config.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "npm run format:check && eslint . && stylelint .",
    "lint-platform": "eslint ./packages/platform && eslint ./packages/ui && stylelint ./packages/platform && stylelint ./packages/ui",
    "lint-mobile": "eslint ./packages/mobile && stylelint ./packages/mobile",
    "type-check": "tsc --noEmit",
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest",
    "test:watch": "npm run test -- --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest",
    "storybook": "storybook dev -p 6006",
    "swagger": "node runSingleTenantSwagger.js --app platform --url http://localhost:8080",
    "swagger-other": "node runSingleTenantSwagger.js --app platform",
    "swagger-mobile": "node runSingleTenantSwagger.js --app mobile",
    "build-storybook": "storybook build",
    "prepare": "./scripts/prepare.sh",
    "pre-commit": "lint-staged",
    "cypress-local": "cypress open --browser chrome",
    "cypress-run-local": "cypress run --browser chrome",
    "cypress-uat": "cypress open --browser chrome --config baseUrl=https://command-uat.willowinc.com",
    "cypress-run-uat": "cypress run --browser chrome --config baseUrl=https://command-uat.willowinc.com",
    "cypress-pvt": "cypress open --browser chrome --config baseUrl=https://command.willowinc.com",
    "cypress-run-pvt": "cypress run --browser chrome --config baseUrl=https://command.willowinc.com",
    "postinstall": "patch-package",
    "generate-translations": "cd scripts/translations && node generate-translation-jsons.js",
    "legacy-ui-usage": "cd scripts/check-legacy-usage && node ./extract-ui-import.js"
  },
