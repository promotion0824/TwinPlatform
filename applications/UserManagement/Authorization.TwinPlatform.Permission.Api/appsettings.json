{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Azure": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Microsoft.AspNetCore.Authentication.JwtBearer": "Error",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
      "Willow": "Information"
    }
  },
  "OpenTelemetry": {
    "AddConsoleExporter": false,
    "AddHttpClientInstrumentation": true,
    "AddAspNetCoreInstrumentation": true
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "should-come-from-extension-settings",
    "CloudRoleName": "authrz-api",
    "Ignore": {
      "Requests": {
        "Names": [],
        "Paths": [ "/swagger", "/health" ]
      },
      "Dependencies": {
        "Names": []
      }
    }
  },
  "ConnectionStrings": {
    "AuthorizationDb": "Connectionstring-goes-here"
  },
  "RunMigrations": true,
  "AzureAd": {
    "Instance": "Comes-From-Manifest",
    "ClientId": "Comes-From-Manifest",
    "TenantId": "Comes-From-Manifest",
    "Audience": "Comes-From-Manifest",
    "AllowWebApiToBeAuthorizedByACL": true
  },
  "GraphAPIApps": [
    {
      "ClientId": "Comes-From-Manifest",
      "TenantId": "Comes-From-Manifest",
      "ClientSecret": "Comes-From-Manifest",
      "Type": "Comes-From-Manifest"
    },
    {
      "ClientId": "Comes-From-Manifest",
      "TenantId": "Comes-From-Manifest",
      "ClientSecret": "Comes-From-Manifest",
      "Type": "Comes-From-Manifest"
    }
  ],
  "HostedServices": {
    "GraphApplicationCacheRefreshService": {
      "Enabled": true,
      "RefreshInterval": "0.00:10:00"
    }
  },
  "Admin": {
    "Admins": [ "Comes-From-Manifest" ]
  },
  "Cache": {
    "AppPermissionCachePeriod": "0.00:05:00",
    "GraphPermissionCachePeriod": "0.00:10:00"
  }
}
