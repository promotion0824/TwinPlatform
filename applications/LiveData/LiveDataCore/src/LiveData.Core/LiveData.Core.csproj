<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <TargetFramework>net8.0</TargetFramework>
    <AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
    <UserSecretsId>02557a75-0623-4619-9445-5641c8c8cb44</UserSecretsId>
    <AssemblyName>Willow.LiveData.Core</AssemblyName>
    <RootNamespace>Willow.LiveData.Core</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <WarningsNotAsErrors>CS0618;NU1900</WarningsNotAsErrors>
