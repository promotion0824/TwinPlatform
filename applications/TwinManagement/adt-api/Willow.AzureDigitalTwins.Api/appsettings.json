{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Azure": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Microsoft.AspNetCore.Authentication.JwtBearer": "Error",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
      "Willow": "Information"
    }
  },
  "OpenTelemetry": {
    "AddConsoleExporter": false,
    "AddHttpClientInstrumentation": true,
    "AddAspNetCoreInstrumentation": true
  },
  "AzureDigitalTwins": {
    "PercentDegreeOfParallelism": 100,
    "SourceType": "Instance",
    "InMemory": {
      "ModelCacheExpirationMinutes": 4320,
      "TwinCacheExpirationMinutes": 60,
      "ExtendedTwinCacheExpirationMinutes": 60,
      "Source": "Empty"
    }
  },
  "ApplicationInsights": {
    "ConnectionString": "should-come-from-extension-settings",
    "CloudRoleName": "twins-api",
    "Ignore": {
      "Requests": {
        "Names": [],
        "Paths": [
          "/swagger",
          "/health",
          "/healthz"
        ]
      },
      "Dependencies": {
        "Names": []
      }
    }
  },
  "ServiceBus": {
    "Topics": {
      "AcsSyncTopic": {
        "ServiceBusName": "CustomerServiceBus",
        "TopicName": "adt-events",
        "SubscriptionName": "acs-sync-subscription"
      },
      "AdxSyncTopic": {
        "ServiceBusName": "CustomerServiceBus",
        "TopicName": "adt-events",
        "SubscriptionName": "adx-sync-subscription"
      }
    }
  },
  "BlobStorage": {
    "Timeout": "0.00:10:00",
    "AsyncContainer": "twins-async",
    "DQRuleContainer": "dq-rule-files",
    "DocumentsContainer": "twindocuments",
    "ResourceId": "Override with Env Variables.Used in AI Search Indexer Datasource. ResourceId will be used as datasource connection string if connectionstring is empty."
  },
  "AzureDataExplorer": {
    "Schema": {
      "DefaultSchemaName": "RealEstatev2",
      "EnableMigration": true,
      "AllowBackfill": false
    }
  },
  "AzureCognitiveSearch": {
    "UnifiedIndexName": "Override with Env Variables",
    "DocumentIndexName": "documents-dev",
    "Uri": "Override with Env Variables"
  },
  "AzureAd": {
    "Instance": "come-from-manifest",
    "ClientId": "come-from-manifest",
    "TenantId": "come-from-manifest",
    "Audience": "come-from-manifest"
  },
  "TimedBackgroundService": {
    "Enabled": true,
    "ScanDelay": "0.00:00:10",
    "Jobs": [
      {
        "JobName": "TwinIncrementalScanJob",
        "Use": "TwinIncrementalScanJob",
        "Enabled": true,
        "RunDelay": "0.00:02:00",
        "QueryBuffer": "00:05:00",
        "QueryPageSize": 100
      },
      {
        "JobName": "TwinScanJob",
        "Use": "TwinScanJob",
        "Enabled": true,
        "RunDelay": "1.00:00:00",
        "Query": "Select * from DigitalTwins",
        "QueryPageSize": 100
      },
      {
        "JobName": "AcsFlushJob",
        "Use": "AcsFlushJob",
        "Enabled": true,
        "RunDelay": "0.00:05:00"
      },
      {
        "JobName": "AdxFlushJob",
        "Use": "AdxFlushJob",
        "Enabled": true,
        "RunDelay": "0.00:05:00"
      },
      {
        "JobName": "MarkSweepDocTwinsJob",
        "Use": "MarkSweepDocTwinsJob",
        "Enabled": true,
        "InitialDelay": "0.00:02:00",
        "RunDelay": "0.06:00:00"
      },
      {
        "JobName": "MarkSweepUnifiedJobCleanup",
        "Use": "MarkSweepUnifiedJobCleanup",
        "Enabled": true,
        "InitialDelay": "0.00:02:00",
        "RunDelay": "0.00:05:00"
      }
    ]
  },
  "TwinJobProcessor": {
    "TwinCustomColumnProcessor": {
      "Enabled": true,
      "Priority": 1
    },
    "TwinAdxSyncProcessor": {
      "Enabled": true,
      "Priority": 2
    },
    "TwinAcsSyncProcessor": {
      "Enabled": false,
      "Priority": 3
    },
    "DocTwinMetadataProcessor": {
      "Enabled": true,
      "Priority": 4
    }
  },
  "ConnectionStrings": {
    "MappingDb": "should-come-from-settings"
  },
  "RunMigrations": true,
  "AISearchIndexerSetup": {
    "SetupEnabled": false,
    "IndexerEnabled": false
  },
  "AzureOpenAIService": {
    "ResourceURI": "Override with Env Variables",
    "DeploymentId": "Override with Env Variables",
    "ApiKey": "Override with Env Variables"
  }
}