{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-TLM",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/Willow.TwinLifecycleManagement.Web/Willow.TwinLifecycleManagement.Web.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "publish-TLM",
      "command": "dotnet",
      "type": "process",
      "args": [
        "publish",
        "${workspaceFolder}/Willow.TwinLifecycleManagement.Web/Willow.TwinLifecycleManagement.Web.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "watch-TLM",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "--project",
        "${workspaceFolder}/Willow.TwinLifecycleManagement.Web/Willow.TwinLifecycleManagement.Web.csproj",
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}