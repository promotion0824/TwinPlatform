{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Willow": "Information",
      "Connector.XL": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Azure": "Warning",
      "Azure.Messaging": "Warning",
      "Azure.Messaging.EventHubs": "Warning",
      "Azure.Core": "Warning",
      "Azure.Storage": "Warning",
      "Azure.Storage.Queues": "Warning",
      "Azure.Storage.Blobs": "Warning",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "",
    "LogLevel": {
      "Default": "Information"
    },
    "CloudRoleName": "ConnectorXL"
  },
  "OpenTelemetry": {
    "AddConsoleExporter": false,
    "AddHttpClientInstrumentation": true,
    "AddAspNetCoreInstrumentation": true,
    "AddSqlClientInstrumentation": false
  },
  "WillowContext": {
    "EnvironmentConfiguration": {
      "ShortName": ""
    },
    "RegionConfiguration": {
      "ShortName": ""
    },
    "StampConfiguration": {
      "Name": ""
    },
    "CustomerInstanceConfiguration": {
      "CustomerSalesId": "",
      "CustomerInstanceName": ""
    }
  },
  "HttpClientFactory": {
    "DirectoryCore": {
      "BaseAddress": "Value from keyvault",
      "Authentication": {
        "Scheme": "TokenFromCookie"
      },
      "RequiresServiceKey": true
    },
    "ConnectorCore": {
      "BaseAddress": "Value from keyvault",
      "Authentication": {
        "Scheme": "TokenFromCookie"
      },
      "RequiresServiceKey": true
    },
    "DigitalTwinCore": {
      "BaseAddress": "Value from keyvault",
      "Authentication": {
        "Scheme": "TokenFromCookie"
      },
      "RequiresServiceKey": true
    },
    "InsightCore": {
      "BaseAddress": "[value from app service setting]",
      "Authentication": {
        "Scheme": "TokenFromCookie"
      },
      "RequiresServiceKey": true
    },
    "LiveDataCore": {
      "BaseAddress": "Value from keyvault",
      "Authentication": {
        "Scheme": "TokenFromCookie"
      },
      "RequiresServiceKey": true
    },
    "SiteCore": {
      "BaseAddress": "Value from keyvault",
      "Authentication": {
        "Scheme": "TokenFromCookie"
      },
      "RequiresServiceKey": true
    },
    "ConnectorImporter": {
      "BaseAddress": "[value from app service setting]",
      "RequiresServiceKey": false
    }
  },

  "EnableSwagger": true,
  "LiveDataHealthCheckTimeThresholdInMins": 40,
  "JsonDotNetSchemaLicense": "",

  "ConnectorExportImportOptions": {
    "StorageAccountName": "[value from app service setting]",
    "StorageKey": "[value from app service setting]",
    "ExportFunctionKey": "[value from app service setting]",
    "ImportFunctionKey": "[value from app service setting]",
    "AsyncImportFunction": {
      "ImportStartKey": "[value from app service setting]",
      "DurableTaskKey": "[value from app service setting]",
      "DurableTaskHub": "[value from app service setting]",
      "DurableTaskConnection": "[value from app service setting]"
    }
  },

  "Auth0": {
    "Domain": "willowtwin-dev.au.auth0.com",
    "Audience": "https://willowtwin-web-dev"
  },

  "AccessControlOptions": {
    "Enabled": false
  },
  "HealthChecks": {
    "Databases": [
    ]
  },
  "DependentHealthChecks": [
    {
      "Name": "ConnectorCore Service",
      "Url": "http://connectorcore/"
    },
    {
      "Name": "DigitalTwinCore Service",
      "Url": "http://digitaltwincore/"
    },
    {
      "Name": "DirectoryCore API",
      "Url": "http://directorycore/"
    },
    {
      "Name": "InsightCore API",
      "Url": "http://insightcore/"
    },
    {
      "Name": "LiveData API",
      "Url": "http://livedatacore/"
    },
    {
      "Name": "SiteCore API",
      "Url": "http://sitecore/"
    }
  ]
}
