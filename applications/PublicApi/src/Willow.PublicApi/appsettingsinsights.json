{
  "ReverseProxy": {
    "Routes": {
      "Insights-Get": {
        "ClusterId": "Insights",
        "Match": {
          "Path": "/v3/insights/{insightid}",
          "Methods": [
            "GET"
          ]
        },
        "AuthorizationPolicy": "InsightsRead",
        "Transforms": [
          {
            "PathPattern": "/insights/{insightid}"
          }
        ]
      },
      "Insights-GetAll": {
        "ClusterId": "Insights",
        "Match": {
          "Path": "/v3/insights",
          "Methods": [
            "POST"
          ]
        },
        "AuthorizationPolicy": "InsightsRead",
        "Transforms": [
          {
            "PathPattern": "/insights"
          }
        ]
      }
    },
    "Clusters": {
      "Insights": {
        "Destinations": {
          "InsightCore": {
            "Address": "http://insightcore/",
            "Swaggers": [
              {
                "PrefixPath": "",
                "AddOnlyPublishedPaths": true,
                "Paths": [
                  "/swagger/v1/swagger.json"
                ]
              }
            ]
          }
        },
        "Metadata": {
          "IsCoreService": "true",
          "HealthCheck": "Federated"
        }
      }
    }
  }
}
