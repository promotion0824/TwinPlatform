{
  "ReverseProxy": {
    "Routes": {
      "Time-Series-MultiTwin": {
        "ClusterId": "Time-Series",
        "Match": {
          "Path": "/v3/time-series/ids",
          "Methods": [
            "POST"
          ]
        },
        "AuthorizationPolicy": "TimeSeriesRead",
        "Transforms": [
          {
            "PathPattern": "/time-series/ids"
          },
          {
            "TwinIdBody": ""
          }
        ]
      },
      "Time-Series-MultiTwin-Lastest": {
        "ClusterId": "Time-Series",
        "Match": {
          "Path": "/v3/time-series/ids/latest",
          "Methods": [
            "POST"
          ]
        },
        "AuthorizationPolicy": "TimeSeriesRead",
        "Transforms": [
          {
            "PathPattern": "/time-series/ids/latest"
          },
          {
            "TwinIdBody": ""
          }
        ]
      },
      "Time-Series-SingleTwin": {
        "ClusterId": "Time-Series",
        "Match": {
          "Path": "/v3/time-series/{twinid}",
          "Methods": [
            "GET"
          ]
        },
        "AuthorizationPolicy": "TimeSeriesRead",
        "Transforms": [
          {
            "PathPattern": "/time-series/{twinid}"
          },
          {
            "TwinIdRoute": "twinid"
          }
        ]
      },
      "Time-Series-SingleTwin-Latest": {
        "ClusterId": "Time-Series",
        "Match": {
          "Path": "/v3/time-series/{twinid}/latest",
          "Methods": [
            "GET"
          ]
        },
        "AuthorizationPolicy": "TimeSeriesRead",
        "Transforms": [
          {
            "PathPattern": "/time-series/{twinid}/latest"
          },
          {
            "TwinIdRoute": "twinid"
          }
        ]
      }
    },
    "Clusters": {
      "Time-Series": {
        "Destinations": {
          "LiveDataCore": {
            "Address": "http://livedatacore/",
            "Swaggers": [
              {
                "PrefixPath": "",
                //"PathFilterRegexPattern": "time-series.*",
                "AddOnlyPublishedPaths": true,
                "Paths": [
                  "/swagger/v1/swagger.json"
                ]
              }
            ]
          }
        },
        "Metadata": {
          "IsCoreService": "true",
          "HealthCheck": "Federated"
        }
      }
    }
  }
}
