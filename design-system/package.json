  "license": "MIT",
  "engines": {
    "node": ">=18.*",
    "npm": ">=10.*"
  },
  "workspaces": [
    "libs/palette",
    "libs/theme",
    "libs/ui",
    "libs/mui-theme"
  ],
  "scripts": {
    "start": "nx storybook ui",
    "build": "nx build ui",
    "changelog": "changeset add",
    "bump-versions": "changeset version && npm install && git add .changeset/* package-lock.json && changeset --empty",
    "type-check-all": "nx run-many --target=type-check --all --parallel",
    "lint-all": "nx run-many --target=lint --all --parallel --skip-nx-cache",
    "test-all": "nx run-many --target=test --all --parallel",
    "check-all": "nx run-many --targets=lint,type-check,test,build,build-storybook,build-playroom --all",
    "e2e": "nx e2e ui-e2e --ui",
    "e2e-updates": "nx e2e ui-e2e --updateSnapshots",
    "e2e-ui-update": "nx e2e ui-e2e --ui --updateSnapshots",
    "publish-packages": "nx run-many --target=publish --projects=tag:ci:publish --parallel=false",
    "custom-install": "node ./tools/scripts/custom-install.js --packages=theme,ui --dir=extensions/real-estate/front-end/Web",
    "generate-component": "nx generate @design-system/plugins:ui-component-generator",
    "generate-dependency-graph": "npx depcruise ./libs/ui/src --include-only \"^libs/ui/src\" --output-type dot | dot -T svg > dependency-graph.svg",
    "bump-copilot-version": "cd libs/copilot && npm version patch && cd ../../",
    "publish-copilot": "nx build copilot && nx publish copilot",
    "playroom": "playroom start --config ./libs/ui/playroom.config.js"
  },
  "private": true,
