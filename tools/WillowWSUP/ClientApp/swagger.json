          "isDevelopment": {
            "type": "boolean"
          },
          "region": {
            "type": "string",
            "nullable": true
          },
          "customerInstanceCode": {
            "type": "string",
            "nullable": true
          },
          "fullCustomerInstanceName": {
            "type": "string",
            "nullable": true
          },
          "lifeCycleState": {
            "$ref": "#/components/schemas/LifeCycleState"
