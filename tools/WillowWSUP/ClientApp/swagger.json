            "type": "string",
            "nullable": true
          },
          "customerInstanceCode": {
            "type": "string",
            "nullable": true
          },
          "applicationName": {
            "type": "string",
            "nullable": true
          },
          "deploymentPhase": {
            "$ref": "#/components/schemas/DeploymentPhase"
          },
          "domain": {
            "type": "string",
            "nullable": true
          },
          "isSingleTenant": {
            "type": "boolean"
          },
          "url": {
            "type": "string",
            "nullable": true
          },
          "healthUrl": {
            "type": "string",
            "nullable": true
          },
          "cloudRoleName": {
            "type": "string",
            "nullable": true
          },
          "applicationInsightsLink": {
            "type": "string",
            "nullable": true
          },
          "applicationInsightsExceptionsLink": {
            "type": "string",
            "nullable": true
