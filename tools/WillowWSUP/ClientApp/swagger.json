        "additionalProperties": false
      },
      "ApplicationSpecification": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "path": {
            "type": "string",
            "nullable": true
          },
          "healthEndpointPath": {
            "type": "string",
            "nullable": true
          },
          "roleName": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CustomerInstance": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "domain": {
            "type": "string",
            "nullable": true
          },
          "deploymentPhase": {
            "$ref": "#/components/schemas/DeploymentPhase"
          },
          "resourceGroup": {
            "type": "string",
            "nullable": true
          },
          "resourceGroupLink": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "subscription": {
            "type": "string",
            "nullable": true
          },
          "isHybrid": {
            "type": "boolean"
